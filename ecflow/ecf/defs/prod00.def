extern /prod18_1/gefs/post_processing
extern /totality_limit:TOTALITY
suite prod00_1
  repeat day 1
  edit ECF_TRIES '1'
  edit ECF_HOME '/gpfs/dell2/emc/modeling/noscrub/Lin.Gan/ecflow-dell/submit'
  edit CYC '00'
  edit MACHINE 'venus'
  edit ENVIR 'test'
  edit PROJ 'GFS'
  edit E 'j'
  #### Developer overwrite
  edit ECF_INCLUDE '/gpfs/dell6/emc/modeling/noscrub/Lin.Gan/gefs-ecflow/gefs.v12.0.0/ecflow/ecf/include'
  edit ECF_OUT '/gpfs/dell2/emc/modeling/noscrub/Lin.Gan/ecflow-dell/logs'
  edit DATAROOT '/gpfs/dell6/stmp/Lin.Gan'
  #### Developer overwrite
  edit QUEUE 'dev'
  edit QUEUESHARED 'dev_shared'
  edit QUEUESERV 'dev_transfer'
  edit ECF_JOB_CMD 'lsub %ECF_JOB% %ECF_JOBOUT% %MACHINE%'
  inlimit /totality_limit:TOTALITY
  family gefs
    edit ECF_FILES '/gpfs/dell6/emc/modeling/noscrub/Lin.Gan/gefs-ecflow/gefs.v12.0.0/ecflow/ecf/scripts/gefs'
    edit PROJENVIR 'DEV'
    edit COM '/gpfs/dell6/ptmp/Lin.Gan/o/nco/ops/com'
    family init
      family atmos
        task jgefs_atmos_getcfssst
        task jgefs_atmos_prep
        task jgefs_atmos_init_recenter
          trigger jgefs_atmos_prep eq complete
      endfamily
      family wave
        task jgefs_wave_init
      endfamily
    endfamily
    family chem
      task jgefs_chem_prep_emissions
      task jgefs_chem_init
        trigger jgefs_chem_prep_emissions eq complete and ../init/atmos/jgefs_atmos_init_recenter eq complete
      task jgefs_chem_forecast
        trigger ../init/atmos/jgefs_atmos_getcfssst eq complete and jgefs_chem_prep_emissions eq complete
      task jgefs_chem_post
        trigger jgefs_chem_forecast eq active or jgefs_chem_forecast eq complete
      task jgefs_chem_prdgen
        trigger jgefs_chem_post eq active or jgefs_chem_post eq complete
    endfamily
    family members
      family c00
        edit MEMBER 'c00'
        task jgefs_forecast_0_16d
          trigger \
            ../../init/atmos/jgefs_atmos_getcfssst eq complete \
            and ../../init/atmos/jgefs_atmos_init_recenter eq complete \
            and wave/jgefs_wave_prep eq complete 
        family atmos
          task jgefs_atmos_post_0_16d
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_prdgen_0_16d
            trigger jgefs_atmos_post_0_16d eq active or jgefs_atmos_post_0_16d eq complete
          task jgefs_atmos_postsnd
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_forecast_16_35d
            trigger ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_post_16_35d
            trigger jgefs_atmos_forecast_16_35d eq active or jgefs_atmos_forecast_16_35d eq complete
          task jgefs_atmos_prdgen_16_35d
            trigger jgefs_atmos_post_16_35d eq active or jgefs_atmos_post_16_35d eq complete
        endfamily
        family wave
          task jgefs_wave_prep
            trigger ../../../init/wave/jgefs_wave_init eq complete
          task jgefs_wave_post
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_wave_gempak
            trigger jgefs_wave_post eq active or jgefs_wave_post eq complete
        endfamily
      endfamily
      family p01
        edit MEMBER 'p01'
        task jgefs_forecast_0_16d
          trigger \
            ../../init/atmos/jgefs_atmos_getcfssst eq complete \
            and ../../init/atmos/jgefs_atmos_init_recenter eq complete \
            and ../../members/c00/wave/jgefs_wave_prep eq complete \
            and wave/jgefs_wave_prep eq complete 
        family atmos
          task jgefs_atmos_post_0_16d
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_prdgen_0_16d
            trigger jgefs_atmos_post_0_16d eq active or jgefs_atmos_post_0_16d eq complete
          task jgefs_atmos_postsnd
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_forecast_16_35d
            trigger ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_post_16_35d
            trigger jgefs_atmos_forecast_16_35d eq active or jgefs_atmos_forecast_16_35d eq complete
          task jgefs_atmos_prdgen_16_35d
            trigger jgefs_atmos_post_16_35d eq active or jgefs_atmos_post_16_35d eq complete
        endfamily
        family wave
          task jgefs_wave_prep
            trigger ../../../init/wave/jgefs_wave_init eq complete
          task jgefs_wave_post
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_wave_gempak
            trigger jgefs_wave_post eq active or jgefs_wave_post eq complete
        endfamily
      endfamily
      family p02
        edit MEMBER 'p02'
        task jgefs_forecast_0_16d
          trigger \
            ../../init/atmos/jgefs_atmos_getcfssst eq complete \
            and ../../init/atmos/jgefs_atmos_init_recenter eq complete \
            and ../../members/c00/wave/jgefs_wave_prep eq complete \
            and wave/jgefs_wave_prep eq complete 
        family atmos
          task jgefs_atmos_post_0_16d
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_prdgen_0_16d
            trigger jgefs_atmos_post_0_16d eq active or jgefs_atmos_post_0_16d eq complete
          task jgefs_atmos_postsnd
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_forecast_16_35d
            trigger ../jgefs_forecast_0_16d eq complete
          task jgefs_atmos_post_16_35d
            trigger jgefs_atmos_forecast_16_35d eq active or jgefs_atmos_forecast_16_35d eq complete
          task jgefs_atmos_prdgen_16_35d
            trigger jgefs_atmos_post_16_35d eq active or jgefs_atmos_post_16_35d eq complete
        endfamily
        family wave
          task jgefs_wave_prep
            trigger ../../../init/wave/jgefs_wave_init eq complete
          task jgefs_wave_post
            trigger ../jgefs_forecast_0_16d eq active or ../jgefs_forecast_0_16d eq complete
          task jgefs_wave_gempak
            trigger jgefs_wave_post eq active or jgefs_wave_post eq complete
        endfamily
      endfamily
    endfamily
    family post_processing
      family atmos
        task jgefs_atmos_prdgen_gfs
        task jgefs_atmos_ensstat_0_16d
          trigger \
            ( ../../members/c00/atmos/jgefs_atmos_prdgen_0_16d eq active or ../../members/c00/atmos/jgefs_atmos_prdgen_0_16d eq complete ) \
            and ( ../../members/p01/atmos/jgefs_atmos_prdgen_0_16d eq active or ../../members/p01/atmos/jgefs_atmos_prdgen_0_16d eq complete ) \
            and ( ../../members/p02/atmos/jgefs_atmos_prdgen_0_16d eq active or ../../members/p02/atmos/jgefs_atmos_prdgen_0_16d eq complete )
        task jgefs_atmos_enspost_0_16d
          trigger \
            ../../members/c00/atmos/jgefs_atmos_prdgen_0_16d eq complete \
            and ../../members/p01/atmos/jgefs_atmos_prdgen_0_16d eq complete \
            and ../../members/p02/atmos/jgefs_atmos_prdgen_0_16d eq complete \
            and jgefs_atmos_prdgen_gfs eq complete
        task jgefs_atmos_ensstat_16_35d
          trigger \
            ( ../../members/c00/atmos/jgefs_atmos_prdgen_16_35d eq active or ../../members/c00/atmos/jgefs_atmos_prdgen_16_35d eq complete ) \
            and ( ../../members/p01/atmos/jgefs_atmos_prdgen_16_35d eq active or ../../members/p01/atmos/jgefs_atmos_prdgen_16_35d eq complete ) \
            and ( ../../members/p02/atmos/jgefs_atmos_prdgen_16_35d eq active or ../../members/p02/atmos/jgefs_atmos_prdgen_16_35d eq complete )
        task jgefs_atmos_enspost_16_35d
          trigger \
            ../../members/c00/atmos/jgefs_atmos_prdgen_16_35d eq complete \
            and ../../members/p01/atmos/jgefs_atmos_prdgen_16_35d eq complete \
            and ../../members/p02/atmos/jgefs_atmos_prdgen_16_35d eq complete
        task jgefs_atmos_ensavg_nemsio
          trigger \
            ( ../../members/c00/jgefs_forecast_0_16d eq active or ../../members/c00/jgefs_forecast_0_16d eq complete ) \
            and ( ../../members/p01/jgefs_forecast_0_16d eq active or ../../members/p01/jgefs_forecast_0_16d eq complete ) \
            and ( ../../members/p02/jgefs_forecast_0_16d eq active or ../../members/p02/jgefs_forecast_0_16d eq complete )
        task jgefs_atmos_avg_postsnd
          trigger jgefs_atmos_ensavg_nemsio eq active or jgefs_atmos_ensavg_nemsio eq complete
      endfamily
      family wave
        task jgefs_wave_stat
          trigger \
            ../../members/c00/wave/jgefs_wave_post eq complete \
            and ../../members/p01/wave/jgefs_wave_post eq complete \
            and ../../members/p02/wave/jgefs_wave_post eq complete
      endfamily
    endfamily
    family gempak
      family atmos
        task jgefs_atmos_gempak
          trigger \
            ( ../../post_processing/atmos/jgefs_atmos_ensstat_0_16d eq active or ../../post_processing/atmos/jgefs_atmos_ensstat_0_16d eq complete ) \
            and ( ../../members/c00/atmos/jgefs_atmos_prdgen_0_16d eq active or ../../members/c00/atmos/jgefs_atmos_prdgen_0_16d eq complete ) \
            and ( ../../members/p01/atmos/jgefs_atmos_prdgen_0_16d eq active or ../../members/p01/atmos/jgefs_atmos_prdgen_0_16d eq complete ) \
            and ( ../../members/p02/atmos/jgefs_atmos_prdgen_0_16d eq active or ../../members/p02/atmos/jgefs_atmos_prdgen_0_16d eq complete )
        task jgefs_atmos_gempak_meta
          trigger jgefs_atmos_gempak eq complete
        task jgefs_atmos_avgspr_gempak_meta
          trigger jgefs_atmos_gempak eq complete
      endfamily
    endfamily
    family cleanup
      task jgefs_atmos_post_cleanup
        trigger \
          ../post_processing/atmos/jgefs_atmos_ensstat_0_16d eq complete \
          and ../post_processing/atmos/jgefs_atmos_enspost_0_16d eq complete \
          and ../post_processing/atmos/jgefs_atmos_ensstat_16_35d eq complete \
          and ../post_processing/atmos/jgefs_atmos_enspost_16_35d eq complete \
          and ../gempak/atmos/jgefs_atmos_gempak_meta eq complete
  endfamily
endsuite
